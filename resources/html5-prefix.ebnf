html = '<html>' head body '</html>'

head = '<head>' normalize-css rend-css title? '</head>'

normalize-css = '<link rel="stylesheet" href="../static/normalize.css">'

rend-css = '<link rel="stylesheet" href="../static/rend.css">'

title = '<title' (<rS> title-attribute)* '>' content* '</title>'

body = '<body' (<rS> body-attribute)* '>' 'x' (element | content)* '</body>'
